{
    "name": "igniteui-js-blocks",
    "version": "0.1.0",
    "license": "MIT",
    "scripts": {
        "postinstall": "npm run typings install && npm prune",
        "start": "npm run build && concurrently \"npm run watch\" \"npm run lite\"",
        "pretest": "npm run build",
        "build": "gulp build",
        "bundle": "gulp bundle && gulp cleanup",
        "watch": "gulp watch",
        "lite": "lite-server",
        "test-dev": "concurrently \"npm run watch\" \"karma start karma.conf.js\"",
        "test": "karma start karma.conf.js --single-run",
        "typings": "node node_modules/typings/dist/bin.js"
    },
    "dependencies": {
        "@angular/common": "^2.0.0",
        "@angular/compiler": "^2.0.0",
        "@angular/core": "^2.0.0",
        "@angular/forms": "^2.0.0",
        "@angular/http": "^2.0.0",
        "@angular/platform-browser": "^2.0.0",
        "@angular/platform-browser-dynamic": "^2.0.0",
        "@angular/router": "^3.0.0",
        "@angular/upgrade": "^2.0.0",
        "core-js": "^2.4.1",
        "reflect-metadata": "^0.1.3",
        "rxjs": "^5.0.1",
        "systemjs": "^0.19.27",
        "zone.js": "^0.7.2",
        "hammerjs": "^2.0.8"
    },
    "devDependencies": {
        "concurrently": "^2.2.0",
        "lite-server": "^2.2.2",
        "typescript": "^2.0.2",
        "typings":"^1.3.2",
        "hammer-simulator": "git+https://github.com/hammerjs/simulator.git",

        "del": "^2.2.2",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^3.1.1",
        "gulp-clean-css": "^2.0.12",
        "gulp-concat": "^2.6.0",
        "gulp-inline-ng2-template": "^3.0.1",
        "gulp-plumber": "^1.1.0",
        "gulp-sass": "^2.3.2",
        "gulp-sourcemaps": "^1.6.0",
        "gulp-typescript": "^2.13.6",
        "gulp-watch": "^4.3.9",
        "jasmine-core": "~2.4.1",
        "coveralls": "^2.11.14",
        "karma": "^1.2.0",
        "karma-chrome-launcher": "^0.2.3",
        "karma-coverage": "^1.1.1",
        "karma-mocha-reporter": "^2.2.0",
        "karma-jasmine": "^0.3.8",
        "material-design-icons": "^3.0.1",
        "merge-stream": "^1.0.0",
        "systemjs-builder": "^0.15.31",
        "ts-node": "^1.3.0",
        "vinyl-paths": "^2.1.0",
        "rimraf": "^2.4.3"
    }
}